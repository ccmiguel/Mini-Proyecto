

<%- include('partials/header', { username: username }) %>

<h1>📄 Posts</h1>
<script src="/js/posts.js"></script>

<% if (posts.length === 0) { %>
  <p>No hay publicaciones todavía.</p>
<% } else { %>
  <% posts.forEach(post => { %>
    <%- include('feed', { post: post }) %>
  <% }) %>
<% } %>

